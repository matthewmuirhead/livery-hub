<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="https://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Livery Hub</display-name>
	<filter>
		<display-name>LiveryConcierge</display-name>
		<filter-name>LiveryConcierge</filter-name>
		<filter-class>com.codemaven.liveries.filter.LiveryConcierge</filter-class>
	</filter>
	<filter-mapping>
		<filter-name>LiveryConcierge</filter-name>
		<url-pattern>*</url-pattern>
	</filter-mapping>
	<error-page>
		<error-code>404</error-code>
		<location>/errors/404.html</location>
	</error-page>
	<session-config>
		<session-timeout>15</session-timeout>
	</session-config>
</web-app>